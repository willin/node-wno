{
  "env": {
	"node": true,
	"mocha": true,
	"es6": true,
	"jquery": true
  },
  "globals": {
	"config": true,
	"db": true,
	"getTimestamp": true,
	"getDefer": true,
	"getPromise": true,
	"jsonResult": true,
	"isArray": true,
	"isEmpty": true,
	"isFile": true,
	"isPromise": true,
	"logger": true,
	"path": true,
	"objSize": true,
	"md5": true,
	"sha1": true
  },
  "rules": {
	"quotes": [
	  2,
	  "single"
	],
	"no-loop-func": 0,
	"no-underscore-dangle": 0,
	"no-extend-native": 0,
	"dot-notation": 0,
	"handle-callback-err": 0
  }
}